

def range(n)
{
	if n == 1 {
		return [0];
	}
	return range(n-1) + [n-1];
}

tower1 = range(3);
tower2 = [];
tower3 = [];



def top(array)
{
	idx = range(length(array) - 1);
	return array[idx];
}



def move(n, a, b, c)
{
	if n == 1
	{
		print("move:", n,  "from", a, " to ", c);
		return;
	}
	move(n-1, a, c, b);
	print("move:", n,  "from", a, " to ", c);
	move(n-1, b, a, c);
}

move(5, "t1", "t2", "t3");


